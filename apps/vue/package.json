{
  "name": "vue",
  "version": "1.0.0",
  "description": "A boilerplate for Full stack VueJS and nestJS and other typescript frameworks too. But i believe you can add JS frameworks too (with little modification)",
  "private": true,
  "author": {
    "name": "Em Ji Madhu",
    "email": "emjimadhu@gmail.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-class-component": "7.2.5",
    "vue-property-decorator": "8.3.0",
    "vue-router": "3.4.3",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-e2e-cypress": "4.5.4",
    "@vue/cli-plugin-router": "4.5.4",
    "@vue/cli-plugin-typescript": "4.5.4",
    "@vue/cli-plugin-unit-jest": "4.5.4",
    "@vue/cli-plugin-vuex": "4.5.4",
    "@vue/cli-service": "4.5.4",
    "@vue/test-utils": "1.0.5",
    "node-sass": "4.13.1",
    "sass-loader": "8.0.0",
    "vue-template-compiler": "2.6.12"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.21.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
